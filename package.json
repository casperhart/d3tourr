{
  "name": "detourr",
  "license": "MIT",
  "dependencies": {
    "three": "^0.128.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/register": "^7.13.16",
    "@types/three": "^0.129.1",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^6.0.0",
    "style-loader": "^3.1.0",
    "ts-loader": "^9.1.2",
    "typescript": "^4.2.4",
    "webpack": "^5.35.1",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^4.0.0-beta.3"
  },
  "scripts": {
    "build": "rm -rf inst/htmlwidgets/lib/scatter_widget && webpack --config ./webpack/package.config.ts && R CMD INSTALL $(git rev-parse --show-toplevel) && R -q -e 'devtools::document(); devtools::build_readme()'",
    "build-dev": "rm -rf dev_build && webpack --config ./webpack/dev.config.ts",
    "start-dev": "yarn webpack serve --config webpack/dev.config.ts --open",
    "clean": "rm -rf dev_build inst/htmlwidgets/lib/scatter_widget",
    "check": "R -q -e 'devtools::check()'",
    "test": "R -q -e 'devtools::test()'"
  },
  "packageManager": "yarn@3.1.1"
}
